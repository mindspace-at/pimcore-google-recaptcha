    _config.googleRecaptcha = {
        debug: {{ google_recaptcha_debug() }},
        publicKey: "{{ google_recaptcha_public_key() }}",
        querySelector: ".google-recaptcha",
        defaultAction: "{{ google_recaptcha_default_action() }}",
    };
</script>
